<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:id="@+id/widget29"
	android:layout_width="fill_parent" android:layout_height="fill_parent"
	android:orientation="vertical" xmlns:android="http://schemas.android.com/apk/res/android">
	<TextView android:id="@+id/widget30" android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:text="Enter Your Current GPA"
		android:layout_gravity="center_horizontal">
	</TextView>
	<EditText android:id="@+id/widget34" android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:text="Enter"
		android:textSize="18sp" android:numeric="decimal"
		android:layout_gravity="center_horizontal">
	</EditText>
	<TextView android:id="@+id/widget33" android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:text="Enter Your Current Attempted Hours"
		android:layout_gravity="center_horizontal">
	</TextView>
	<EditText android:id="@+id/widget32" android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:text="Enter"
		android:textSize="18sp" android:numeric="decimal"
		android:layout_gravity="center_horizontal">
	</EditText>
	<TextView
android:id="@+id/directions"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:text="Enter the Course Hours and Grade that you      expect to get this semester"
android:textStyle="bold|italic"
>
</TextView>
	
	<TableLayout android:id="@+id/widget48"
		android:layout_width="fill_parent" android:layout_height="210px"
		android:orientation="vertical">
		<TableRow android:id="@+id/widget52" android:layout_width="fill_parent"
			android:layout_height="40px" android:orientation="horizontal">
			<TextView android:id="@+id/widget53" android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:text="Course 1 Hours:">
			</TextView>
			<Spinner android:id="@+id/hours1" android:layout_width="75px"
				android:layout_height="40px">
			</Spinner>
			<TextView android:id="@+id/widget55" android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:text="Grade:">
			</TextView>
			<Spinner android:id="@+id/grade1" android:layout_width="75px"
				android:layout_height="40px">
			</Spinner>
		</TableRow>
		<TableRow android:id="@+id/widget57" android:layout_width="fill_parent"
			android:layout_height="wrap_content" android:orientation="horizontal">
			<TextView android:id="@+id/widget58" android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:text="Course 2 Hours:">
			</TextView>
			<Spinner android:id="@+id/hours2" android:layout_width="75px"
				android:layout_height="40px">
			</Spinner>
			<TextView android:id="@+id/widget61" android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:text="Grade:">
			</TextView>
			<Spinner android:id="@+id/grade2" android:layout_width="75px"
				android:layout_height="40px">
			</Spinner>
		</TableRow>
		<TableRow android:id="@+id/widget63" android:layout_width="fill_parent"
			android:layout_height="wrap_content" android:orientation="horizontal">
			<TextView android:id="@+id/widget64" android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:text="Course 3 Hours:">
			</TextView>
			<Spinner android:id="@+id/hours3" android:layout_width="75px"
				android:layout_height="40px">
			</Spinner>
			<TextView android:id="@+id/widget66" android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:text="Grade:">
			</TextView>
			<Spinner android:id="@+id/grade3" android:layout_width="75px"
				android:layout_height="40px">
			</Spinner>
		</TableRow>
		<TableRow android:id="@+id/widget68" android:layout_width="fill_parent"
			android:layout_height="wrap_content" android:orientation="horizontal">
			<TextView android:id="@+id/widget69" android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:text="Course 4 Hours:">
			</TextView>
			<Spinner android:id="@+id/hours4" android:layout_width="75px"
				android:layout_height="40px">
			</Spinner>
			<TextView android:id="@+id/widget71" android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:text="Grade:">
			</TextView>
			<Spinner android:id="@+id/grade4" android:layout_width="75px"
				android:layout_height="40px">
			</Spinner>
		</TableRow>
		<TableRow android:id="@+id/widget73" android:layout_width="fill_parent"
			android:layout_height="wrap_content" android:orientation="horizontal">
			<TextView android:id="@+id/widget74" android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:text="Course 5 Hours:">
			</TextView>
			<Spinner android:id="@+id/hours5" android:layout_width="75px"
				android:layout_height="40px">
			</Spinner>
			<TextView android:id="@+id/widget76" android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:text="Grade:">
			</TextView>
			<Spinner android:id="@+id/grade5" android:layout_width="75px"
				android:layout_height="40px">
			</Spinner>
		</TableRow>
	</TableLayout>
	<Button android:id="@+id/widget35" android:layout_width="100px"
		android:layout_height="50px" android:text="Calculate"
		android:layout_gravity="center_horizontal">
	</Button>
</LinearLayout>
